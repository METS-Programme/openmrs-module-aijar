	<script type="text/javascript">


		function enable_disable_fp(enableDisablePregnant){

		var chkd =
		$j(enableDisablePregnant).find(":checkbox").attr('checked');

		$j(".EnableDisablePregnantChild").find("input").attr("disabled",chkd);

		}


		function enable_disable_ar(enableDisable_adherenceReason){

		var
		adherence_combo = $j(enableDisable_adherenceReason).find('select');

		var selected_value =
		jQuery.trim(adherence_combo.find(":selected").text());

		var disable =
		selected_value.toUpperCase()
		=="GOOD"
		$j(".EnableDisableAdherenceReasonChild").find("input").attr("disabled",disable
		);

		}


		function enable_disable_fpm(enableDisable_fpmethod){

		var fp_combo =
		$j(enableDisable_fpmethod).find('select');

		var selected_value =
		jQuery.trim(fp_combo.find(":selected").text());

		var disable =
		selected_value.toUpperCase()
		!="YES"
		$j(".EnableDisablefpmethodchild").find("input").attr("disabled",disable
		);

		}


		function enable_disable_tb(enableDisable_tbstatus){

		var tb_combo =
		$j(enableDisable_tbstatus).find('select');

		var selected_value =
		jQuery.trim(tb_combo.find(":selected").text());

		var disable =
		selected_value.toUpperCase()
		!="4. TB RX"
		$j(".EnableDisabletbstatuschild").find("input").attr("disabled",disable
		);

		}


		function enable_disable_r(enableDisable_regimen){


		var regimen_combo
		= $j(enableDisable_regimen).find('select');

		var regimen_value =
		jQuery.trim(regimen_combo.find(":selected").text());

		var disable =
		regimen_value.toUpperCase()
		!= "OTHER"


		$j(".EnableDisableRegimenChild").find("input").attr("disabled",disable
		);
		}


		if(jQuery){
		$j(document).ready(function(){



		if ( $j.browser.msie ) {
		$j(":checkbox").click(function(){
		$j(this).change();
		});
		}

		var
		patientGender = "
		<lookup expression='patient.gender' />
		";
		$j("#w22-display").removeAttr("disabled");
		$j("#w22").removeAttr("disabled");
		$j("#isPregant").prop("disabled",true);

		//if patient.gender = F,
		if
		(patientGender == 'F') {
		// display pregancy details

		$j( "#w20" ).on(
		"click", function() {
		if($j(this).attr("checked") == "checked"){
		//enable
		$j("#isPregant").show();
		}else{
		//hide
		$j("#isPregant").hide();
		}
		});


		}else if (patientGender == 'M') {
		//hide Pregnancy details
		$j("#pregnacy").hide();

		//hide family planning options[Oral C,Hormone,
		Diaphram, IUD]
		$j("#womenFamilyPlanningMethod").hide();
		}


		// disable the
		return-date input
		$j('#w8-display').attr("disabled", "disabled");

		//
		When the scheduled-date field is selected, then set the minimum value
		of the return-date
		$j('#w5-display').change(function() {
		// enable the
		return date picker
		$j('#w8-display').removeAttr("disabled", "");

		//
		clear the return date so that a new one can be selected
		$j('#w8-display').val("");
		$j('#w8').val("");

		// set the minimum date
		using the schedule date
		$j('#w8-display').datepicker("option",
		"minDate",
		$j('#w5-display').val());

		});

		// when there is a value in the
		scheduled date field
		if ($j('#w5-display').val().length > 0) {
		// set
		the minimum date using the return date
		$j('#w8-display').datepicker("option", "minDate",
		$j('#w5-display').val());
		}



		$j('#w50-display').attr("disabled",
		"disabled");

		// When the scheduled-date field is selected, then set the
		minimum value
		of the return-date
		$j('#w50-display').change(function() {
		// enable the return date picker
		$j('#w52-display').removeAttr("disabled", "");

		// clear the return date
		so that a new one can be selected
		$j('#w52-display').val("");
		$j('#w52').val("");

		// set the minimum date using the schedule date
		$j('#w52-display').datepicker("option", "minDate",
		$j('#w50-display').val());
		});

		// when there is a value in the scheduled
		date field
		if ($j('#w50-display').val().length > 0) {
		// set the minimum
		date using the return date
		$j('#w52-display').datepicker("option",
		"minDate",
		$j('#w50-display').val());
		}

		$j(".EnableDisablePregnant").change(function() {

		enable_disable_fp(this);

		});


		$j(".EnableDisablePregnant").each(function() {

		enable_disable_fp(this);
